{
  "name": "sixofone.pushred.co",
  "author": "Eric Lanehart <eric@pushred.co> (http://pushred.co)",
  "scripts": {
    "dev": "env-cmd ./.env.json gulp $1",
    "deploy": "npm run task deploy",
    "deploy:functions": "cd tasks && apex deploy --env-file ../.env.json",
    "task": "cd tasks && env-cmd ../.env.json node $1",
    "task:gulp": "env-cmd ./.env.json gulp $1",
    "lint": "semistandard",
    "lint:staged": "semistandard $(git status --porcelain | grep -E '(M|A)?(\\s\\s|M|A).*.js$' | awk '{print $2}' | tr '\n' ' ')"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pushred/sixofone.pushred.co.git"
  },
  "engines": {
    "node": ">=6.5.0",
    "npm": ">=3.10.3"
  },
  "babel": {
    "presets": ["es2015"]
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "chalk": "^1.1.3",
    "env-cmd": "^2.2.0",
    "envify": "^3.4.1",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.2",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-s3-upload": "^1.6.1",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.0.0",
    "gulp-watch": "^4.3.9",
    "gulplog": "^1.0.0",
    "image-type": "^2.1.0",
    "lost": "^7.1.0",
    "postcss-apply": "^0.4.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "postcss-nesting": "^2.3.1",
    "run-sequence": "^1.2.2",
    "sharp": "^0.16.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "optionalDependencies": {
    "browser-sync": "^2.16.0",
    "clean-urls": "^1.1.5",
    "semistandard": "^9.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.6.4",
    "dom-delegate": "^2.0.3",
    "lodash": "^4.15.0",
    "npm": "^3.10.7",
    "parameterize": "0.0.7",
    "pouchdb": "^6.0.5",
    "shortid": "^2.2.6",
    "unorphan": "^1.2.1",
    "word-wrap": "^1.1.0"
  }
}
